    "release": "np --no-2fa",
    "test": "vitest run",
    "test-dev": "vitest watch"
  },
  "author": "arnoson",
  "license": "MIT",
  "keywords": [
    "very",
    "simple",
    "router",
    "tiny",
    "small",
    "es6",
    "esnext",
    "vanilla",
    "SPA"
  ],
  "devDependencies": {
    "happy-dom": "^6.0",
    "np": "^7.6.1",
    "shx": "^0.3.4",
    "tsup": "^6.0",
    "typescript": "^4.8.4",
    "vite": "^3.0",
    "vitest": "^0.23"
  }
}
