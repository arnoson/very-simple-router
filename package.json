{
  "name": "@very-simple/router",
  "version": "0.2.1",
  "description": "A very simple router",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/very-simple/router.git"
  },
  "scripts": {
